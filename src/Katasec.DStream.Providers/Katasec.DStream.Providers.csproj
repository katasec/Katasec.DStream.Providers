<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <!-- NuGet metadata -->
    <PackageId>Katasec.DStream.Providers</PackageId>
    <Version>0.0.1-dev</Version>
    <Authors>Ameer Deen</Authors>
    <Company>Katasec</Company>   
    <Description>Standard input and output providers for DStream plugins</Description>
    <RepositoryUrl>https://github.com/katasec/Katasec.DStream.Providers</RepositoryUrl>
    <PackageTags>dstream;plugin;providers</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../../../Katasec.DStream.Plugins/src/Katasec.DStream.Plugin/Katasec.DStream.Plugin.csproj" />
  </ItemGroup>

</Project>
